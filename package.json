{
  "name": "angular-stats",
  "version": "1.2.4",
  "description": "Little utility to show stats about your angularjs app",
  "author": "Maurizio Cescon <maurizio.cescon@gmail.com>",
  "homepage": "https://be.linkedin.com/in/mauriziocescon",
  "private": false,
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=10.1.0"
  },
  "main": "./dist/angular-stats.js",
  "types": "./interfaces/index.d.ts",
  "scripts": {
    "prepublish": "webpack --profile --color --env name=prod",
    "build": "webpack --progress --profile --color --env name=prod",
    "pretest": "webpack --progress --profile --color --env name=test",
    "test": "karma start karma.conf.js",
    "bundle-analyzer": "webpack-bundle-analyzer ./dist/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mauriziocescon/angularjs-stats"
  },
  "keywords": [
    "analysis",
    "angularjs",
    "cescon",
    "digest",
    "maurizio",
    "measure",
    "stats",
    "watcher"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/brentertz/scapegoat/blob/master/LICENSE-MIT"
    }
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@types/angular": "^1.8.8",
    "@types/angular-mocks": "^1.7.4",
    "@types/jasmine": "^5.1.2",
    "@types/node": "^24.2.1",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "angular": "^1.8.3",
    "angular-mocks": "^1.8.3",
    "babel-loader": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "^9.10.0",
    "eslint-webpack-plugin": "^4.2.0",
    "jasmine-core": "^5.1.1",
    "jasmine-node": "^3.0.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-spec-reporter": "^0.0.36",
    "source-map-loader": "^5.0.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.5.3",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^6.0.1",
    "webpack-merge": "^6.0.1"
  }
}

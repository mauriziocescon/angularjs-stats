{
  "name": "angular-stats",
  "version": "1.2.4",
  "description": "Little utility to show stats about your angularjs app",
  "author": "Maurizio Cescon <maurizio.cescon@gmail.com> (http://www.mauriziocescon.com)",
  "homepage": "http://www.mauriziocescon.com",
  "private": false,
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=10.1.0"
  },
  "main": "./dist/angular-stats.js",
  "types": "./interfaces/index.d.ts",
  "scripts": {
    "prepublish": "webpack --profile --color --env name=prod",
    "build": "webpack --progress --profile --color --env name=prod",
    "pretest": "webpack --progress --profile --color --env name=test",
    "test": "karma start karma.conf.js",
    "bundle-analyzer": "webpack-bundle-analyzer ./dist/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mauriziocescon/angularjs-stats"
  },
  "keywords": [
    "analysis",
    "angularjs",
    "cescon",
    "digest",
    "maurizio",
    "measure",
    "stats",
    "watcher"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/brentertz/scapegoat/blob/master/LICENSE-MIT"
    }
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@types/angular": "^1.8.8",
    "@types/angular-mocks": "^1.7.4",
    "@types/jasmine": "^5.0.0",
    "@types/node": "^20.0.0",
    "@types/webpack": "^5.0.0",
    "angular": "1.8.3",
    "angular-mocks": "1.8.3",
    "babel-loader": "^9.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "jasmine-core": "^5.0.0",
    "jasmine-node": "^3.0.0",
    "karma": "^6.0.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-jasmine": "^5.0.0",
    "karma-spec-reporter": "0.0.36",
    "source-map-loader": "^4.0.0",
    "ts-loader": "^9.0.0",
    "tslint": "^6.0.0",
    "tslint-loader": "^3.0.0",
    "typescript": "^5.0.0",
    "webpack": "^5.0.0",
    "webpack-bundle-analyzer": "^4.0.0",
    "webpack-cli": "^5.0.0",
    "webpack-merge": "^5.0.0"
  }
}
